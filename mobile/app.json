{
  "expo": {
    "name": "Sahada",
    "slug": "sahadamobile",
    "owner": "yerdogan",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "icon": "./assets/icon.png",
    "web": {
      "bundler": "metro"
    },
    "ios": {
      "bundleIdentifier": "com.yerdogan.sahadamobile",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.sahada.mobile"
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "./plugins/withBoringSSLFix"
    ],
    "extra": {
      "eas": {
        "projectId": "916c86b4-56c3-4f8c-b150-78c4d8f23342"
      }
    }
  }
}
