{
  "name": "SahadaMobile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "cd android && ./gradlew clean && cd ../ios && xcodebuild clean",
    "pod-install": "cd ios && pod install",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace SahadaMobile.xcworkspace -scheme SahadaMobile -configuration Release"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@notifee/react-native": "^7.8.2",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/geolocation": "^3.2.1",
    "@react-native-firebase/app": "^19.0.1",
    "@react-native-firebase/firestore": "^19.0.1",
    "@react-native-firebase/messaging": "^19.0.1",
    "@react-native-firebase/storage": "^19.3.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "axios": "^1.6.5",
    "date-fns": "^3.0.6",
    "expo": "^54.0.33",
    "expo-clipboard": "~8.0.8",
    "firebase": "^12.9.0",
    "react": "18.2.0",
    "react-dom": "^18.3.1",
    "react-native": "0.73.2",
    "react-native-biometrics": "^3.0.1",
    "react-native-contacts": "^7.0.8",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-image-picker": "^7.1.0",
    "react-native-mmkv": "^2.11.0",
    "react-native-reanimated": "3.6.3",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-share": "^10.0.2",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "^0.21.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/runtime": "^7.23.7",
    "@react-native/babel-preset": "^0.73.19",
    "@react-native/eslint-config": "^0.73.2",
    "@react-native/metro-config": "^0.73.3",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.48",
    "@types/react-test-renderer": "^18.0.7",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "patch-package": "^8.0.1",
    "prettier": "^3.1.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
